{  
  "version": 1,  
  "disable_existing_loggers": false,  
  "formatters": {  
    "simple": {  
      "format": "%(asctime)s - %(name)s - [%(levelname)s] - %(message)s"
  }  
  },  
  "handlers": {  
    "log_json": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "INFO",
      "formatter": "simple",
      "filename": "logs/logging.log",
      "maxBytes": 10240,
      "backupCount": 5,
      "encoding": "utf8"
  }
  },  
  "root": {  
    "level": "INFO",
    "handlers": ["log_json"]
  }  
}