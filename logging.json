{  
  "version": 1,  
  "disable_existing_loggers": false,
  "formatters": {  
    "simple": {  
      "format": "[%(levelname)s]: %(name)s: [%(asctime)s] - %(message)s"
  }  
  },  
  "handlers": {  
    "log_json": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "INFO",
      "formatter": "simple",
      "filename": "logs/logging.log",
      "maxBytes": 102400,
      "backupCount": 5,
      "encoding": "utf8"
  }
  },  
  "root": {  
    "level": "INFO",
    "handlers": ["log_json"]
  }  
}